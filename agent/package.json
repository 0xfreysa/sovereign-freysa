{
  "name": "@ghostagent/testagent111",
  "packageManager": "pnpm@8.15.1",
  "version": "0.0.32",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts && cp src/chat/schema.graphql dist/",
    "dev": "tsup src/index.ts --watch",
    "lint": "eslint --fix --cache 'src/**/*.ts'",
    "clean": "rm -rf dist",
    "prettier": "prettier --write --cache .",
    "grpcMockEthSigner": "tsx src/common/mocks/grpcMockEthSigner.ts",
    "test": "jest",
    "generate": "protoc --proto_path=../proto --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/common/generated/proto --ts_proto_opt=outputServices=grpc-js,env=node,esModuleInterop=true ../proto/key_pool.proto",
    "generate:chat": "graphql-codegen",
    "migrate": "node-pg-migrate --migrations-dir ./src/chat/storage/migrations",
    "build:chat": "tsup src/chat/standalone.ts --format cjs --dts --clean --out-dir dist/chat && cp src/chat/schema.graphql dist/chat/",
    "start:chat": "node dist/chat/standalone.cjs",
    "dev:chat": "tsx src/chat/standalone.ts",
    "prepublishOnly": "npm run generate:chat && npm run build",
    "pack": "npm run generate:chat && npm run build && npm pack"
  },
  "files": [
    "dist",
    "src",
    "src/chat/generated",
    "dist/schema.graphql"
  ],
  "dependencies": {
    "@ai-sdk/openai": "^1.0.13",
    "@aws-sdk/client-s3": "^3.741.0",
    "@fastify/cors": "^10.0.2",
    "@fastify/middie": "^9.0.3",
    "@safe-global/api-kit": "^2.5.6",
    "@safe-global/protocol-kit": "^5.1.1",
    "@safe-global/types-kit": "^1.0.1",
    "ai": "^4.0.27",
    "better-sqlite3": "^11.7.2",
    "ethers": "^6.13.5",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "graphql": "^16.10.0",
    "jose": "^5.9.6",
    "mercurius": "^16.0.1",
    "neo4j-driver": "^5.27.0",
    "node-cron": "^3.0.3",
    "node-pg-migrate": "^7.9.0",
    "openai": "^4.77.3",
    "pg": "^8.13.1",
    "pgvector": "^0.2.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "sqlite-vec": "0.1.7-alpha.2",
    "twitter-api-v2": "^1.19.0",
    "uuid": "^11.0.5",
    "viem": "^2.22.17"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^2.2.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-resolvers": "^4.4.1",
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.7.13",
    "@types/better-sqlite3": "^7.6.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.7",
    "grpc-tools": "^1.12.4",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.2.5",
    "ts-proto": "^2.6.1",
    "tsup": "^8.3.5",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
